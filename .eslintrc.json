{
	"plugins": ["@typescript-eslint"],
	"extends": ["next", "prettier", "plugin:@typescript-eslint/recommended"],
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"prefer-const": "error",
		"quotes": ["error", "single"],
		"semi": ["error", "never"],
		"@typescript-eslint/no-unused-vars": "error",
		"@typescript-eslint/no-explicit-any": "error"
	}
}
